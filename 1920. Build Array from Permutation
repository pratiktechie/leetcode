class Solution {
    public int[] buildArray(int[] nums) {
    
        // ArrayList<Integer> arr=new ArrayList<Integer>();
        int[] arr=new int[nums.length];
        for(int i=0; i<nums.length;i++){
            int sam=nums[i];
            int amp=nums[sam];
            // arr.add(amp);
            for(int z=0;z<nums.length;z++){
                arr[i]=amp;
            }
        }
      return arr;
    }
}
