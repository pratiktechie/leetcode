class Solution {

    public static void main(String[] args) {
        boolean ans = isPalindrome(121);
        System.out.println(ans);
    }
    static boolean isPalindrome(int x) {

        int temp=x;
        int rev=0;
        int rem;

        while(x != 0){

            rem = x%10;
            rev = rev*10+rem;
            x = x/10;
        }

        return (rev==temp);

    }
}
